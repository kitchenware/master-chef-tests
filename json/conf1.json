{
  "repos": {
    "git": [
      "http://github.com/octo-technology/master-chef.git"
    ]
  },
  "run_list": [
    "recipe[base::system]",
    "recipe[master_chef::chef_solo_scripts]",
    "recipe[confluence]",
    "recipe[logrotate]"
  ],
  "node_config": {
    "confluence": {
      "location": "/toto",
      "tomcat": {
        "connectors": {
          "http": {
            "port": 9999,
            "address": "127.0.0.1"
          }
        }
      }
    },
    "logrotate" : {
      "fake" : {"chef" : ["/home/chef/fake.log"]}
    }
  }
}