{
  "repos": {
    "git": [
      "git://github.com/octo-technology/master-chef.git"
    ]
  },
  "run_list": [
    "recipe[base::system]",
    "recipe[master_chef::chef_solo_scripts]",
    "recipe[tomcat]",
    "recipe[mysql]",
    "recipe[php5::apache2]",
    "recipe[php5::phpinfo]",
    "recipe[rails]"
  ],
  "node_config": {
    "additional_packages": ["build-essential", "libmysqlclient-dev"],
    "ssh_keys": {
      "deploy": {
        "users": "user1",
        "keys": ["ssh-rsa  master-chef"]
      }
    },
    "nginx": {
      "default_vhost": {
        "listen": "0.0.0.0:81"
      }
    },
    "php5": {
      "apache2": {
        "default_mimetype": "abcd123"
      },
      "modules": ["mysql"],
      "pear_modules": ["cache_lite"]
    },
    "tomcat": {
      "instances": {
        "my_instance": {
          "connectors": {
            "http": {
              "address": "0.0.0.0",
              "port": 8080
            }
          }
        }
      }
    },
    "mysql": {
      "databases": {
        "db_test": {
          "host": "localhost",
          "username": "toto",
          "password": "titi"
        }
      }
    },
    "rails_app": {
      "conf4": {
        "user": "user1",
        "app_directory": "/toto",
        "mysql_database": "mysql:databases:db_test"
      }
    }
  }
}